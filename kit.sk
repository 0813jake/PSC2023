        if {coin.%arg-1%} > 15:
            subtract 15 from {coin.%arg-1%}
            give 1 golden apple to arg-1
        else:
            message "&e돈이 부족합니다." to arg-1

command /oneitem4_8 [<player>]:
    trigger:
        if {coin.%arg-1%} > 30:
            subtract 30 from {coin.%arg-1%}
            give 1 enchanted golden apple to arg-1
        else:
            message "&e돈이 부족합니다." to arg-1